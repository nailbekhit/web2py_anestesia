                                     INPUT(_id=\'radio_laringoscopia_sim\',\n                                                      _type=\'radio\',\n                                                      _name=\'laringoscopia\',\n                                                      _value=\'T\'),\n                                                \'sim\',\n                                                INPUT(_id=\'radio_laringoscopia_nao\',\n                                                      _type=\'radio\',\n                                                      _name=\'laringoscopia\',\n                                                      _value=\'F\'),\n                                                \'n\xc3\xa3o\',\n                                                BR(),\n                                                \'Broncofibroscopia: \',\n                                                INPUT(_id=\'radio_broncofibroscopia_sim\',\n                                                      _type=\'radio\',\n                                                      _name=\'broncofibroscopia\',\n                                                      _value=\'T\'),\n                                                \'sim\',\n                                                INPUT(_id=\'radio_broncofibroscopia_nao\',\n                                                      _type=\'radio\',\n                                                      _name=\'broncofibroscopia\',\n                                                      _value=\'F\'),\n                                                \'n\xc3\xa3o\',\n                                                BR(),\n                                                \'Estilete luminoso: \',\n                                                INPUT(_id=\'radio_est_luminoso_sim\',\n                                                      _type=\'radio\',\n                                                      _name=\'estilete_luminoso\',\n                                                      _value=\'T\'),\n                                                \'sim\',\n                                                INPUT(_id=\'radio_est_luminoso_nao\',\n                                                      _type=\'radio\',\n                                                      _name=\'estilete_luminoso\',\n                                                      _value=\'F\'),\n                                                \'n\xc3\xa3o\',\n                                                BR(),\n                                                \'Videolaringoscopia: \',\n                                                INPUT(_id=\'radio_videolaringoscopia_sim\',\n                                                      _type=\'radio\',\n                                                      _name=\'videolaringoscopia\',\n                                                      _value=\'T\'),\n                                                \'sim\',\n                                                INPUT(_id=\'radio_videolaringoscopia_nao\',\n                                                      _type=\'radio\',\n                                                      _name=\'videolaringoscopia\',\n                                                      _value=\'F\'),\n                                                \'n\xc3\xa3o\',\n                                            )\n                                        ),\n                                        BR(),\n                                    )\n                                ),\n                                HR(),\n                                BR(),\n                                DIV(_id=\'div_agentes\',\n                                    *FIELDSET(\n                                        LEGEND(_id=\'lgd_agentes\',\n                                               *\'Agentes\'),\n                                        \'Nome: \',\n                                        INPUT(_type=\'text\', _id=\'agente1\'),\n                                        INPUT(_type=\'button\', _id=\'btn_inclui_agente\', _value=\'+\'),\n                                    )),\n                                BR(),\n                                HR(),\n                                FIELDSET(\n                                    LEGEND(_id=\'lgd_dados_cirurgia\',\n                                           *\'DADOS CIRURGIA\'),\n                                    \'Cirurgia: \',\n                                    INPUT(_type=\'text\', _id=\'cirurgia\', _name=\'cirurgia\'),\n                                    \'Anestesista: \',\n                                    INPUT(_type=\'text\', _id=\'anestesia\', _name=\'anestesia\'),\n                                    \'Cirurgi\xc3\xa3o: \',\n                                    INPUT(_type=\'text\', _id=\'cirurgiao\', _name=\'cirurgiao\'),\n                                ),\n                                BR(),\n                                HR()\n                            )\n\n                        )\n                    )\n                )\n            )\n        )\n    return campos\n\n\ndef index():\n    myconsulta = \'\'\n    linhas = \'\'\n    dados = \'\'\n    qtd_linhas = 0\n    busca = gerar_campos_busca()\n\n    campo_h = request.vars.campo_hidden_escolha_busca\n    if busca.accepts(request, session):\n        if request.vars.campo_hidden_escolha_busca == \'1\':\n            linhas = consulta_nome(request.vars.nome_busca)\n            qtd_linhas = conta_consulta(request.vars.nome_busca, 1)\n        elif request.vars.campo_hidden_escolha_busca == \'2\':\n            linhas = consulta_registro(request.vars.nr_registro_busca)\n            qtd_linhas = conta_consulta(request.vars.nome_busca, 2)\n\n    return dict(busca=busca, linhas=linhas, nr_linhas=qtd_linhas)\n\nresponse._vars=response._caller(carrega_dados)\n'
